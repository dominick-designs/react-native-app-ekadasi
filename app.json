{
  "expo": {
    "name": "ekadasi",
    "slug": "ekadasi",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ekadasi",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dominickdesigns.ekadasi",
      "buildNumber": "0.0.5",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.dominickdesigns.ekadasi",
      "useNextNotificationsApi": true,
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}